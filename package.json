{
    "name": "@cursorsdottsx/docgen",
    "version": "1.0.0",
    "description": "Maintained version of @discordjs/docgen in TypeScript.",
    "main": "dist/index.js",
    "bin": {
        "ts-docgen": "dist/index.js"
    },
    "scripts": {
        "watch": "tsc -w",
        "exec": "ts-node src/index.ts --source test/src/ --output test/out/data.json"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/cursorsdottsx/docgen.git"
    },
    "keywords": [
        "discord.js",
        "documentation",
        "docs",
        "generator",
        "docgen",
        "docsgen",
        "discord",
        "api",
        "library"
    ],
    "author": "Amish Shah <amishshah.2k@gmail.com>",
    "contributors": [
        "Schuyler Cebulskie <Gawdl3y@Gawdl3y.com>",
        "cursorsdottsx <cursors.dev@gmail.com>"
    ],
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/cursorsdottsx/docgen/issues"
    },
    "homepage": "https://github.com/cursorsdottsx/docgen/#readme",
    "dependencies": {
        "js-yaml": "^3.13.1",
        "jsdoc-to-markdown": "^5.0.1",
        "tsubaki": "^1.3.2",
        "yargs": "^14.0.0"
    },
    "devDependencies": {
        "@types/js-yaml": "^4.0.1",
        "@types/jsdoc-to-markdown": "^7.0.0",
        "@types/node": "^15.12.2",
        "@types/yargs": "^17.0.0",
        "tslib": "^2.3.0"
    }
}
